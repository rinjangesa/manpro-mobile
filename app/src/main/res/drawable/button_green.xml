<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape>
            <stroke android:width="1dp" android:color="#cfffc4" />
            <corners android:radius="5dp" />
            <solid android:color="#fdfdfd" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="1dp" android:color="#d1ffc4" />
            <corners android:radius="5dp" />
            <solid android:color="#fdfdfd" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="#8dff76" />
            <corners android:radius="5dp" />
            <solid android:color="#73ca5f" />
        </shape>
    </item>
</selector>